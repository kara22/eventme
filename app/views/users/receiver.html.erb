<div class="card-attende">
  <%= image_tag @attendee.facebook_picture_url, class: "receiver-picture"%>
  <div class="receiver-name">
  <p><%= @attendee.first_name %></p>
  </div>
  <div id="dislike">
    <%= image_tag 'croix.png',width:40 %>
  </div>
  <div id="like">
    <%= image_tag 'heart.png', width:40 %>
  </div>
</div>
<div>
  <% current_user.events.each do |event| %>
    <% if event.end_time %>
      <% if @attendee.events.include?(event) && DateTime.parse(event.end_time) < Time.now %>
        <p><%= event.name %></p>
      <% end %>
    <% end %>
  <% end %>
</div>
